/* Timeline root. */
.wtfAppUiTimeline {
  @mixin fitParent();
  background: white;
  border-bottom: 1px solid #EBEBEB;
}

.wtfAppUiTimelineOuter {
  position: absolute;
  width: 100%;
  height: 100%;
}

.wtfAppUiTimelineCanvas {
}
