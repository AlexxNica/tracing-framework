/**
 * Navbar DOM.
 *
 * @author benvanik@google.com (Ben Vanik)
 */

{namespace wtf.app.ui.nav.navbar}


/**
 * Root control UI.
 */
{template .control}
  <div class="{css wtfAppUiNavbar}">
    <div class="{css wtfAppUiNavbarTimeline}"></div>
    <div class="{css wtfAppUiNavbarFramebar}"></div>
    <div class="{css wtfUiSplitterHorizontal} {css wtfAppUiNavbarSplitter}"></div>
    <div class="{css kScrollShadowTop}"></div>
    <div class="{css kScrollShadowBottom}"></div>
  </div>
{/template}
