/**
 * Resizable control CSS.
 *
 * @author benvanik@google.com (Ben Vanik)
 */

.wtfUiSplitterHorizontal {
  position: absolute;
  height: 8px;
  left: 0;
  right: 0;
  bottom: 0;
  margin-bottom: -3px;
  cursor: ns-resize;
}
